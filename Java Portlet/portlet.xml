			<keywords>SHSG-SimulacaoApp-PV</keywords>
		</portlet-info>
		<supported-processing-event>
			<name>contratacaoappEvent</name>
		</supported-processing-event>

	</portlet>
	
	<default-namespace>http://SHSG-SimulacaoApp-PV/</default-namespace>
	
	<event-definition>
		<description>contratacaoappEvent</description>
		<name>contratacaoappEvent</name>
		<alias xmlns:customns="http://SHSG-App-PV">customns:objeto</alias>
		<value-type>java.util.Hashtable</value-type>
	</event-definition>

</portlet-app>